<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IRemoteWorkerService" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:28756/RemoteWorkerService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IRemoteWorkerService"
                contract="WSClient.IRemoteWorkerService" name="BasicHttpBinding_IRemoteWorkerService" />
        </client>
        <behaviors>
            <serviceBehaviors>
                <behavior name="">
                    <serviceMetadata httpGetEnabled="true" httpsGetEnabled="true" />
                    <serviceDebug includeExceptionDetailInFaults="false" />
                </behavior>
            </serviceBehaviors>
        </behaviors>
        <services>
            <service name="NetReduce.CoordinatorService.CoordinatorService">
                <endpoint address="" binding="basicHttpBinding" contract="NetReduce.CoordinatorService.ICoordinatorService">
                    <identity>
                        <dns value="localhost" />
                    </identity>
                </endpoint>
                <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
                <host>
                    <baseAddresses>
                        <add baseAddress="http://localhost:8733/Design_Time_Addresses/NetReduce.CoordinatorService/CoordinatorService/" />
                    </baseAddresses>
                </host>
            </service>
        </services>
    </system.serviceModel>
</configuration>